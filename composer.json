{
    "name": "weblement/yii2",
    "description": "Yii2 extensions and other useful components for yii2 applications.",
    "keywords": ["weblement", "yii2", "extensions", "behaviors", "widgets", "components"],
    "homepage": "https://github.com/weblement/",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Yuv Joodhisty",
            "email": "locustv2@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/weblement/yii2/issues?state=open",
        "source": "https://github.com/weblement/yii2"
    },
    "require": {
        "yiisoft/yii2": "~2.0.4"
    },
    "autoload" : {
        "psr-4" : {
            "weblement\\" : "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
